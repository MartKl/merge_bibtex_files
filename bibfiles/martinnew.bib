@Article{GanRecYam11,
  Title                    = {Tensor completion and low-n-rank tensor recovery via convex optimization},
  Author                   = {Silvia Gandy and Benjamin Recht and Isao Yamada},
  Journal                  = {Inverse Problems},
  Year                     = {2011},
  Number                   = {2},
  Pages                    = {025010},
  Volume                   = {27},
  Abstract                 = {In this paper we consider sparsity on a tensor level, as given by the n-rank of a tensor. In an important sparse-vector approximation problem (compressed sensing) and the low-rank matrix recovery problem, using a convex relaxation technique proved to be a valuable solution strategy. Here, we will adapt these techniques to the tensor setting. We use the n-rank of a tensor as a sparsity measure and consider the low-n-rank tensor recovery problem, i.e. the problem of finding the tensor of the lowest n-rank that fulfills some linear constraints. We introduce a tractable convex relaxation of the n-rank and propose efficient algorithms to solve the low-n-rank tensor recovery problem numerically. The algorithms are based on the Douglas-Rachford splitting technique and its dual variant, the alternating direction method of multipliers.},
  Url                      = {http://stacks.iop.org/0266-5611/27/i=2/a=025010},
}


@Article{GraFerCor15,
  Title                    = {Accelerated randomized benchmarking},
  Author                   = {Christopher Granade and Christopher Ferrie and D G Cory},
  Journal                  = {New J. Phys.},
  Year                     = {2015},
  Number                   = {1},
  Pages                    = {013042},
  Volume                   = {17},

  Abstract                 = {Quantum information processing offers promising advances for a wide range of fields and applications, provided that we can efficiently assess the performance of the control applied in candidate systems. That is, we must be able to determine whether we have implemented a desired gate, and refine accordingly. Randomized benchmarking reduces the difficulty of this task by exploiting symmetries in quantum operations. Here, we bound the resources required for benchmarking and show that, with prior information, we can achieve several orders of magnitude better accuracy than in traditional approaches to benchmarking. Moreover, by building on state-of-the-art classical algorithms, we reach these accuracies with near-optimal resources. Our approach requires an order of magnitude less data to achieve the same accuracies and to provide online estimates of the errors in the reported fidelities. We also show that our approach is useful for physical devices by comparing to simulations.},
  Url                      = {http://stacks.iop.org/1367-2630/17/i=1/a=013042}
}

